    "keywords": ["redit", "cron", "lock"],
    "type": "library",
    "homepage" : "https://github.com/AlexDisler/MutexLock",
    "authors": [
        {
            "name": "Alex Disler",
            "homepage": "http://github.com/AlexDisler"
        }
    ],
    "support": {
        "issues": "https://github.com/AlexDisler/MutexLock"
    },
    "require": {
        "php": ">=5.4",
        "colinmollenhour/credis": "~1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*"
    },    
    "autoload": {
        "psr-0": {
            "MutexLock": "lib/"
        }
    }
}
